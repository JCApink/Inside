<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.inside.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.test.inside.model.pojo.User">
        <id column="id" property="id" />
        <result column="userName" property="userName" />
        <result column="password" property="password" />
        <result column="email" property="email" />
        <result column="sex" property="sex" />
        <result column="imageName" property="imageName" />
        <result column="state" property="state" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, userName, password, email, sex, imageName, state
    </sql>

</mapper>
